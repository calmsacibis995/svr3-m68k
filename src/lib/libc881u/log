	# compile MC68881-dependent library modules
	cd ../libc881/m68k; make -e all CC=cc FP=m68881u		 DBLALIGN=NO   
	# PROCESSOR = M68020
	# DBLALIGN  = NO
	# FP        = m68881u
	cd crt ; make -e  nonprof
fladd.s:
	m4 ../../../libc/m68k/m4.def -DMCOUNT=#  fladd.s   > temp.s   && \
	cc -O -I../../../libc/m68k/crt -c temp.s && mv temp.o fladd.o
