cp dmp /usr/bin
cp serv /usr/bin
cp servon /usr/bin
cp servoff /usr/bin
cp download /u/service
cp upload /u/service
cp hard /u/service
cp soft /u/service
cp site_log /u/service
cp site_data /u/service
cp dotprofile /u/service/.profile
chgrp sys /usr/bin/serv
chgrp sys /usr/bin/servon
chgrp sys /usr/bin/servoff
chgrp sys /u/service/download
chgrp sys /u/service/upload
chgrp 99 /u/service/.profile
chown root /usr/bin/serv
chown root /usr/bin/servon
chown root /usr/bin/servoff
chown root /u/service/download
chown root /u/service/upload
chown service /u/service/.profile
chmod 111 /usr/bin/dmp
chmod 4111 /usr/bin/serv
chmod 4111 /usr/bin/servon
chmod 4111 /usr/bin/servoff
chmod 4111 /u/service/download
chmod 4111 /u/service/upload
chmod 644  /u/service/.profile
